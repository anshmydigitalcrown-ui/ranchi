{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next@latest"
    }
  ],
  "redirects": [
    {
      "source": "/rates",
      "destination": "/services",
      "permanent": true
    },
    {
      "source": "/booking",
      "destination": "/contact", 
      "permanent": true
    },
    {
      "source": "/testimonials",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/rates/(.*)",
      "destination": "/services",
      "permanent": true
    },
    {
      "source": "/booking/(.*)",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/testimonials/(.*)",
      "destination": "/about", 
      "permanent": true
    }
  ]
}