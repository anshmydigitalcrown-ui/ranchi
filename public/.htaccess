# Additional redirect rules for broken links
RewriteEngine On

# Permanent redirects for broken URLs
RewriteRule ^rates/?$ /services [R=301,L]
RewriteRule ^booking/?$ /contact [R=301,L]
RewriteRule ^testimonials/?$ /about [R=301,L]

# Handle various URL variations
RewriteRule ^rates.html$ /services [R=301,L]
RewriteRule ^booking.html$ /contact [R=301,L]
RewriteRule ^testimonials.html$ /about [R=301,L]

# Security headers
Header always set X-Frame-Options "SAMEORIGIN"
Header always set X-Content-Type-Options "nosniff"